{
  "manifest_version": 3,
  "name": "TimeTrack - Browsing Activity Tracker",
  "version": "1.0.0",
  "description": "Track your browsing time and get insightful analytics",
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "alarms"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/dist/index.html"
  }
}
